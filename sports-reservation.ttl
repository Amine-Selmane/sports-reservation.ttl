@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/ontologies/sports-reservation#> .

# Classe Utilisateur
ex:Utilisateur a rdfs:Class ;
    rdfs:subClassOf [
        rdf:type rdfs:Class ;
        rdfs:label "Client"
    ], [
        rdf:type rdfs:Class ;
        rdfs:label "Admin"
    ], [
        rdf:type rdfs:Class ;
        rdfs:label "Coach"
    ], [
        rdf:type rdfs:Class ;
        rdfs:label "Personnel"
    ], [
        rdf:type rdfs:Class ;
        rdfs:label "PartenairesEtSponsors"
    ] .

# Classe installations_sportives
ex:InstallationsSportives a rdfs:Class ;
    rdfs:subClassOf [
        rdf:type rdfs:Class ;
        rdfs:label "Terrain"
    ], [
        rdf:type rdfs:Class ;
        rdfs:label "SalleDeSport"
    ], [
        rdf:type rdfs:Class ;
        rdfs:label "Equipement"
    ] .

